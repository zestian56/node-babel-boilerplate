{
  "name": "babel-node-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate project for a nodejs babel, mocha and chai aplication.",
  "main": "index.js",
  "scripts": {
    "start": "babel-node src/index.js",
    "dev": "nodemon --exec babel-node src/index.js",
    "build": "babel src --out-dir ./dist --source-maps",
    "serve": "node ./dist/index.js",
    "test": "./node_modules/.bin/mocha --require @babel/register"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.3",
    "@babel/register": "^7.4.0",
    "chai": "^4.2.0",
    "mocha": "^6.1.4",
    "nodemon": "^1.18.11"
  },
  "dependencies": {
    "dotenv": "^7.0.0"
  },
  "engines": {
    "node": ">=8.0.0 <11.0.0"
  }
}
